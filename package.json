{
  "name": "snabbdom-material",
  "version": "0.6.0",
  "description": "Stateless UI components for snabbdom that follow material design",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "lint": "standard src/**/*.js app/**/*.js test/**/*.js --verbose | snazzy",
    "pretest": "npm run lint",
    "test": "mocha",
    "start": "BABEL_DISABLE_CACHE=1 parallelshell 'watch \"npm run lint --silent\" src app test' 'babel --watch src --out-dir lib' 'babel-node app/server'",
    "prebuild": "npm run lint",
    "build": "rimraf lib && babel src --out-dir lib && rimraf dist && NODE_ENV=production webpack --config webpack.production.config.js --progress --profile --colors",
    "prepublish": "npm run build"
  },
  "standard": {
    "global": [
      "React"
    ],
    "ignore": [
      "/app/vendor/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/garth/snabbdom-material.git"
  },
  "keywords": [
    "appbar",
    "button",
    "calendar",
    "cerebral",
    "checkbox",
    "component",
    "datepicker",
    "dialog",
    "form",
    "grid",
    "input",
    "material design",
    "material",
    "material-components",
    "menu",
    "snabbdom",
    "select",
    "sidenav",
    "table"
  ],
  "author": "Garth Williams",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/garth/snabbdom-material/issues"
  },
  "homepage": "https://github.com/garth/snabbdom-material#readme",
  "dependencies": {
    "deepmerge": "^0.2.10",
    "easy-style": "^1.0.0",
    "exenv": "^1.2.0",
    "insert-css": "^0.2.0",
    "moment": "^2.11.2",
    "node-waves": "^0.7.5",
    "snabbdom": "^0.3.4"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "babel-cli": "^6.6.4",
    "babel-core": "^6.6.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-react-jsx": "^6.6.4",
    "babel-plugin-webpack-loaders": "^0.3.8",
    "babel-preset-es2015": "^6.6.0",
    "baobab": "^2.3.3",
    "bootstrap-sass": "^3.3.6",
    "cerebral": "^0.33.27",
    "cerebral-addons": "^0.5.2",
    "cerebral-model-baobab": "^0.4.7",
    "cerebral-module-router": "^0.13.1",
    "cerebral-view-snabbdom": "^0.4.1",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.2.0",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.9.0",
    "mocha": "^2.4.5",
    "node-sass": "^3.4.2",
    "normalize.css": "^3.0.3",
    "parallelshell": "^2.0.0",
    "postcss-loader": "^0.8.1",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.1.2",
    "snabbdom-jsx": "^0.3.0",
    "snazzy": "^3.0.0",
    "standard": "^6.0.7",
    "stats-webpack-plugin": "^0.3.1",
    "style-loader": "^0.13.0",
    "watch": "^0.17.1",
    "webpack": "^1.12.13",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.9.1",
    "webpack-sources": "^0.1.1"
  }
}
